// Based on https://docs.aws.amazon.com/eventbridge/latest/userguide/eb-event-patterns-content-based-filtering.html#eb-filtering-complex-example
{
  "Event": {
    "id": "1",
    "source": "test-source",
    "detail-type": "test-detail-type",
    "account": "123456789012",
    "region": "us-east-2",
    "time": "2022-07-13T13:48:01Z",
    "detail": {
      "location": "eu-central-1"
    }
  },
  "EventPattern": {
    "time": [ { "prefix": "2017-10-02" } ],
    "detail": {
      // if you include a key more than once the last reference will be the one used to evaluate events
      "location": [ { "prefix": "us-" } ],
      "location": [ { "anything-but": "us-east" } ]
    }
  }
}
